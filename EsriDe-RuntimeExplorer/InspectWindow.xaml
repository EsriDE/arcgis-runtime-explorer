<Window x:Class="EsriDe.RuntimeExplorer.InspectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EsriDe.RuntimeExplorer"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        DataContext="{StaticResource Locator}"
        Title="InspectWindow" Height="768" Width="1024">
    <Grid>
        <xctk:PropertyGrid SelectedObject="{Binding}" />
    </Grid>
</Window>
